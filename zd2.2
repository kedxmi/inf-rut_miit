t1 = 1  # Вводим значение переменных действующих с результатом функции
t2 = 1  # Вводим значение переменных действующих с результатом функции
a = int(input('Введите значение a:'))  # Вводим значение а
b = int(input('Введите значение b:'))  # Вводим значение b
while t1 != 0 and t2 != 0:  # Используем оператор while для задания
    for i in range(-10, 11):  # Ввели перебор переменной i
        if b > 1:  # Условие действий
            t1 = ((a-(b*i))*4)
            t2 = (i*((a**2)+(3*b*i)+(4*(i**2))))
            if t1 != 0 and t2 != 0:  # условие для нормального вывода программы
                print('Если i=', i)
                print('Результат функции:', t1/t2)
                t1 = 0  # Обнуляем переменные чтобы не было повторов вывода и программа работала стабильно
                t2 = 0  # Обнуляем переменные чтобы не было повторов вывода и программа работала стабильно
        if b <= 1:  # Условие действий
            t1 = ((a+(b*i))*4)
            t2 = (i*((a**2)+(3*(1-b)*i)+(4*(i**2))))
